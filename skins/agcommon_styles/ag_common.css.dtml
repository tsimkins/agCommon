/* YOUR CSS RULES START HERE */

/* = body =================================================== */

body {
    background-color: &dtml-bodyBackgroundColor;;
    color: &dtml-globalFontColor;;
    background-repeat: repeat-x;
    font-size: 100%;
    font-family: Verdana, Arial, Helvetica, sans-serif;
}

/* = visual-portal-wrapper ================================== */

#visual-portal-wrapper {
    margin: 1.25em auto 3em auto;
    padding: 0 1.25em;
    max-width: &dtml-maxWidth;;
}

/* = portal-top ============================================= */

#portal-top {
    margin: 0;
    background-color: &dtml-siteHeaderBackgroundColor;;
}

/* = portal-header ========================================= */

#portal-header {
    margin: 0 auto;
    padding: 1em 1.25em 1.75em;
    max-width: &dtml-maxWidth;;
    text-align: left;
}

#portal-header a {
    border-width: 0;
    text-decoration: none;
}


/* = portal-searchbox ====================================== */

/*
    These are all in browser/resources/ad54elements-style.css
*/


/* = logo-psu ============================================== */

#logo-psu {
    display: block;
    width: 44.052863436123%;
}

#logo-psu.subsite {
    width: 190px;
}

#logo-psu img, #logo-college img {
    width: 100%;
}

/* = logo-college ========================================== */

#logo-college {
    display: block;
    height: 40px;
    left: 121px;
    position: absolute;
    top: 40px;
    width: 72px
}


/* = logo-print ============================================ */

#logo-print {
    display: none;
}

/* = portal-breadcrumbs ==================================== */

#portal-breadcrumbs {
    background-color: transparent;
    color: &dtml-breadcrumbTextColor;;
    font-weight: normal;
    font-size: 1.125em;
    letter-spacing: 0.025em;
    padding: 0;
    border-bottom: 0;
    margin: 1em 0;
    line-height: 1.125em;
}

#portal-breadcrumbs .breadcrumbSeparator {
    color: &dtml-breadcrumbSeparatorColor;;
}

#portal-breadcrumbs>span {
    font-size: 0.625em;
}

#portal-breadcrumbs #breadcrumbs-current {
    font-weight: bold;
}

#portal-breadcrumbs a {
    color: &dtml-breadcrumbLinkColor;;
    font-weight: normal;
}

#portal-breadcrumbs a:hover {
    text-decoration: underline;
}

/* = section-title ========================================= */

#section-title {
    font-size: 2em;
    font-weight: bold;
    margin: 0 0 0.75em 0;
}

#section-title a {
    color: &dtml-sectionTitleTextColor;;
    padding-bottom:  0;
    border-bottom: 1px solid &dtml-sectionTitleBorder;;
}

#section-title a:hover {
    color: &dtml-breadcrumbLinkColor;;
    text-decoration: none;
    border-bottom-color: &dtml-breadcrumbLinkColor;;
}

/* - Begin autogenerated grid.css ------------------------------------------- */

/* = portal-columns ========================================================= */
#portal-columns {
  background-color: &dtml-contentBackgroundColor;;
  max-width: 1000px;
  margin: 0 auto;
  padding: 1.25em 0.8%;
  width: 98.4%;
  vertical-align: top;
  display: table; }

/* = portal-column-one and portal-column-two ================================ */
#portal-column-one, #portal-column-two {
  margin: 0;
  padding: 0 1.62602%;
  width: 21.74797%;
  vertical-align: top;
  display: table-cell; }

/* = portal-column-content ================================================== */
#portal-column-content {
  margin: 0;
  padding: 0 1.62602%;
  width: 96.74797%;
  vertical-align: top;
  display: table-cell; }

#portal-column-content.sl,
#portal-column-content.sr {
  width: 71.74797%; }

#portal-column-content.sl.sr {
  width: 46.74797%; }

/* - End autogenerated grid.css --------------------------------------------- */


/* ================= Portlet Styles =======================*/

.portletWrapper {
    border: none;
    padding: 0;
    margin: 0 0 1.5em 0;
    font-size: 0.875em;
}

#content .portletWrapper {
    font-size: 1em;
}

/* ================= Portlet headers ===================== */

.portletHeader,
#content h2.portletHeader {
    background-color: transparent;
    border-color: &dtml-rightPortletHeaderBorderColor;;
    border-style: solid;
    border-width: 0 0 1px 0;
    color: &dtml-rightPortletHeaderTextColor;;
    font-family: Arial, Helvetica, Helv, sans-serif;
    font-size: 1.375em;
    font-weight: normal;
    line-height: 1.375em;
    margin: 0 0 0.375em 0;
    padding: 0;
    text-align: left;
    text-transform: none;
}

#portal-column-one .portletHeader,
#portal-column-one .portletHeader span,
#portal-column-one .portletHeader a,
#portal-column-one .portletHeader a:hover {
    color: &dtml-leftPortletHeaderTextColor;;
}


.portletHeader span,
.portletHeader a,
.portletHeader a:hover {
    color: &dtml-rightPortletHeaderTextColor;;
}

.portletHeader a {
    color: &dtml-rightPortletHeaderTextColor;;
    margin: 0;
    text-decoration: none;
    border: none;
}

/* ================= Portlet item =====================*/

.portlet .portletItem:first-child {
    border-width: 0 0 0 0;
}

.portletItem,
.portletItem,
.portletFooter,
.portletError dd {
    background-color: transparent;
    border-width: 0;
    color: &dtml-rightPortletTextColor;;
    display: block;
    margin: 0 0 1em 0;
    padding: 0;
}

.portletItem p {
    padding: 0;
}

.portletItem a.title {
    line-height: 1.25em;
    font-weight: normal;
    margin-top: 0;
    border: none;
    display: block;
}

.portletItem a.title:hover {
    text-decoration: underline;
}

.portletItem .summary {
    color: &dtml-rightPortletSummaryColor;;
    font-size: 0.75em;
    margin: 0.125em 0 0 0;
    font-weight: normal;
    text-align: left;
    line-height: 1.5em;
}

.portletItemDetails {
    color: &dtml-rightPortletDiscreteTextColor;;
    font-size: 0.75em;
    font-weight: normal;
    text-align: left;
    line-height: 1.5em;
    margin-bottom: 0;
    line-height: 1.375em;
    border: none;
    box-shadow: none;
    width: auto;
    padding: 0;
}

.portletfeedmixer .portletItem .summary {
    margin-top: 0.375em;
    color: &dtml-rightPortletSummaryColor;;
}

.portletItem a.more {
    display: block;
    font-size: 0.875em;
    margin: 5px;
    text-align: left;
    text-decoration: none;
    border: none;
}

/* ================= Portlet Footer =====================*/

.portletFooter {
    background-color: transparent;
    padding: 0;
    font-size: 0.875em;
    border: none;
    margin: 0.25em 0;
    text-align: left;
}

/* ================= Portlet Type =====================*/

/* ----------------- Static Text --------------------- */

.portletStaticText .portletItem {
    padding: 0 0 0.75em 0;
    font-size: 0.90625em;
    margin: 0;
}


.portletStaticText .portletItem p {
    margin-bottom: 0.375em;
}

/* Portlet */

.portlet {
    border-bottom: none;
    background-color: transparent;
}

.portletItem ul {
    margin: 0;
    padding: 0;
}

.portletItem li {
    list-style-image: none;
    list-style-type: none;
    margin: 0 0 0.25em;
