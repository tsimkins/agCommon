.portletLinkIcon .sprite a {
    background-image: url(&dtml-portal_url;/linkicon-sprites.png);
    background-repeat: no-repeat;
    min-height: 24px;
    display: block;
}

.portletLinkIcon .sprite a span {
    margin-left: 30px;
    vertical-align: middle;
    display: block;
}

.portletLinkIcon {
    margin-top: 0.75em;
    margin: 0 2.162162162162162% 1.5em; /* 4/185 */
}

.portletLinkIcon .portletItem {
    line-height: 1.1875em;
    margin: 0.875em 0;
    padding: 0;
    background-color: transparent;
    min-height: 24px;
}

.portletLinkIcon a {
    text-decoration: none;
    color: &dtml-leftLinkTextColor;;
    display: block;
    border: none;
}

.portletLinkIcon a:hover span {
    text-decoration: underline;
    color: &dtml-leftLinkTextHoverColor;;
}

.portletLinkIcon img {
    width: 24px;
    padding-right: 6px;
    color: &dtml-leftLinkTextHoverColor;;
    margin: 0;
    vertical-align: middle;
}

/* Link Buttons */

.portletLinkButton {
    padding: 0;
}

.portletLinkButton .portletItem a,
.portletLinkButton .portletItem a:hover {
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    background-color: &dtml-leftButtonBackground_default;;
    border-radius: 4px;
    border: 1px solid &dtml-leftButtonBorder_default;;
    border-bottom: 1px solid &dtml-leftButtonBorder_default;;
    color: &dtml-leftButtonTextColor_default;;
    display: block;
    font-size: 0.875em;
    font-size: 0.875rem;
    font-weight: normal;
    list-style-image: none;
    list-style-type: none;
    margin: 0 0 1em 0;
    padding: 0.5em 5%;
