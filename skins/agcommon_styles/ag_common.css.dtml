#portal-top-navigation li:first-child a {
    border-width: 0;
    padding-left: 0.75em;
}

#portal-top-navigation a {
    color:&dtml-topnavigationTextColor;;
    display: inline;
    color: &dtml-topnavigationTextColor;;
    text-decoration: none;
    border: none;
    line-height: 1.6875em;
}

#portal-top-navigation li a:hover {
    background-image: none;
    background-color: &dtml-topnavigationBackgroundHoverColor;;
    color: &dtml-topnavigationTextHoverColor;;
}


/* ----- portlets ------------------------------------------ */

.portletItem {
    border: none;
    background-color: transparent;
    border-width: 0;
    color: &dtml-rightPortletTextColor;;
    padding: 0;
    margin: 0.25em 0 1.125em 0;
}

.portletItem ul {
    margin: 0;
    padding: 0;
}

.portletItem ul.indent {
