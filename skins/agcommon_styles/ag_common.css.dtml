    text-decoration: none;

}

.portletLinkButton dd.alternate a,
.portletLinkButton dd.alternate a:hover {
    border: 1px solid &dtml-leftButtonBorder_alternate;;
    background-color: &dtml-leftButtonBackground_alternate;;
    color: &dtml-leftButtonTextColor_alternate;;
}

.portletLinkButton img {
    width: 100%;
    display: block;
}

/* = portal-top-navigation ================================= */

#portal-top-navigation {
    margin: 0 auto;
    padding: 0;
    background-color: &dtml-topnavigationBackgroundColor;;
}

#portal-top-navigation.empty {
    border-bottom: none;
