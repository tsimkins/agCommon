    text-decoration: underline;
    border-bottom: none;
}

.portletLinkIcon .portletItem a:hover,
.portletLinkIcon .portletItem a:hover {
    text-decoration: none;
}

.portletItem a,
.portletItem a:visited,
.portlet dd a:visited {
    color: &dtml-rightPortletLinkColor;;
    text-decoration: none;
    border: none;
    font-weight: normal;
}

.portletfeedmixer .summary {
    line-height: 1.5em;
}

/* manage Custom Portlets */
.customManagePortletsLink {
