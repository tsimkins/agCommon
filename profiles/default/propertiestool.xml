<?xml version="1.0"?>
<object name="portal_properties">
    <object name="agcommon_properties" meta_type="Plone Property Sheet">
        <property name="title">agCommon Properties</property>
        <property name="footer_copyright" type="string" purge="False">College of Agricultural Sciences</property>
        <property name="footer_copyright_link" type="string">http://agsci.psu.edu</property>
        <property name="enable_addthis" type="boolean">True</property>
        <property name="enable_fblike" type="boolean">True</property>
        <property name="site_rss_title" type="string">Penn State College of Agricultural Sciences News</property>
        <property name="site_rss_link" type="string">http://live.psu.edu/tagrss/Agricultural_Sciences</property>
        <property name="site_header_options" type="lines">
            <element value="penn_state"/>
            <element value="college"/>
            <element value="department"/>
        </property>
        <property name="department_header_options" type="lines">
            <element value="department"/>
            <element value="other"/>
            <element value="none"/>
        </property>
        <property name="department_name_style_options" type="lines">
            <element value="two_line"/>
            <element value="one_line"/>
            <element value="none"/>
        </property>
        <property name="site_header" 
                  select_variable="site_header_options"
                  type="selection">
                    college
        </property>
        <property name="department_header" 
                  select_variable="department_header_options"
                  type="selection">
                    none
        </property>
        <property name="department_name_style" 
                  select_variable="department_name_style_options"
                  type="selection">
                    none
        </property>
  </object>
</object>
